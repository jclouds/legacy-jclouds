            name="Red Hat Enterprise Linux 5 (64-bit)"
            type="application/vnd.tmrk.cloud.operatingSystem"/>
    <Description/>
    <Processor>
        <Minimum>1</Minimum>
        <Maximum>8</Maximum>
        <StepFactor>1</StepFactor>
    </Processor>
    <Memory>
